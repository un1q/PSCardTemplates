{
    "width": 410,
    "multi": [{
        "CSV": "attack.front.A.csv",
        "template": "templates\\customTemplate.svg",
        "countColumn": "count",
        "outputDir": "generated\\attack\\frontA",
        "atlasPath": "generated\\attack\\attack.atlas.A",
        "replace": [{
                "search": "#ffeebb",
                "value": "#c0c0ff"
            }, {
                "search": "%name%",
                "column": "name"
            }, {
                "search": "%type%",
                "value": "attack"
            }, {
                "search": "%text%",
                "column": "text"
				}
        ]
    },{
        "CSV": "attack.front.B.csv",
        "template": "templates\\customTemplate.svg",
        "countColumn": "count",
        "outputDir": "generated\\attack\\frontB",
        "atlasPath": "generated\\attack\\attack.atlas.B",
        "replace": [{
                "search": "#ffeebb",
                "value": "#c0c0ff"
            }, {
                "search": "%name%",
                "column": "name"
            }, {
                "search": "%type%",
                "value": "attack"
            }, {
                "search": "%text%",
                "column": "text"
				}
        ]
    }],
    "back": {
        "CSV": null,
        "template": "templates\\customBack.svg",
        "countColumn": null,
        "outputDir": "generated\\attack",
        "atlasPath": null,
        "replace": [{
                "search": "sodipodi:sides=\"5\"",
                "value": "sodipodi:sides=\"2\""
            }, {
                "search": "%type%",
                "value": "attack"
            },{
                "search": "#ffeebb",
                "value": "#c0c0ff"
            }
        ]
    }
}