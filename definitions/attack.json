{
    "width": 408,
    "front": {
        "CSV": "attack.front.csv",
        "template": "templates\\customTemplate.svg",
        "countColumn": "count",
        "outputDir": "generated\\attack\\front",
        "atlasPath": "generated\\attack\\attack.atlas",
        "replace": [{
                "search": "#ffeebb",
                "value": "#c0c0ff"
            }, {
                "search": "%type%",
                "value": "attack"
            }, {
                "search": "%name%",
                "column": "name"
            }, {
                "search": "%text%",
                "column": "text"
            }
        ]
    },
    "back": {
        "CSV": null,
        "template": "templates\\customBack.svg",
        "countColumn": null,
        "outputDir": "generated\\attack",
        "atlasPath": null,
        "replace": [{
                "search": "sodipodi:sides=\"5\"",
                "value": "sodipodi:sides=\"2\""
            }, {
                "search": "%type%",
                "value": "attack"
            }, {
                "search": "#ffeebb",
                "value": "#c0c0ff"
            }
        ]
    }
}
