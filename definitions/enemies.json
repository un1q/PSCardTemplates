{
    "width": 408,
    "front": {
        "CSV": "enemies.front.csv",
        "template": "templates\\5linesTemplate.svg",
        "countColumn": "count",
        "outputDir": "generated\\enemies\\front",
        "atlasPath": "generated\\enemies\\enemies.atlas",
        "replace": [{
                "search": "#ffeebb",
                "value": "#ff9343"
            }, {
                "search": "%name%",
                "column": "name"
            }, {
                "search": "%type%",
                "value": "aggro"
            }, {
                "search": "%line1%",
                "value": "attack 1"
            }, {
                "search": "%line2%",
                "column": "attack1"
            }, {
                "search": "%line3%",
                "value": " "
            }, {
                "search": "%line4%",
                "value": "attack 2"
            }, {
                "search": "%line5%",
                "column": "attack2"
            }
        ]
    },
    "back": {
        "CSV": null,
        "template": "templates\\customBack.svg",
        "countColumn": null,
        "outputDir": "generated\\enemies",
        "atlasPath": null,
        "replace": [{
                "search": "sodipodi:sides=\"5\"",
                "value": "sodipodi:sides=\"4\""
            }, {
                "search": "%type%",
                "value": "enemy"
            }, {
                "search": "#ffeebb",
                "value": "#ff9343"
            }
        ]
    }
}
